@{
    Layout = null;

    var seriesTitle = "Harry Potter";
    var issueNumber = 700;
    var description = "<p>Harry Potter is a wizzard abc xyz </p>";
    var artists = new string[] {
        "Harry",
        "Hermionie",
        "Ron"
    };
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <div>
        <h1>@seriesTitle</h1>
        <h2>#@issueNumber</h2>
        <h5>Desciption: </h5>
        <div>@Html.Raw(description)</div>
        <h5>Artists</h5>
        <div>
            <ul>
                @foreach (var artist in artists)
                {
                    <li> @artist</li>
                }
            </ul>
           
        </div>
    </div>
</body>
</html>
